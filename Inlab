#include<iostream>
using namespace std;
int fact(int n)
{
	     if (n==1 || n==0)
        {
          return 1;
        }
        else
        {
          return n*fact(n-1);
        }

}
int main()
{
	int n;

	cin>>n;

	string ch;
	for (int i=0;i<n;i++)
	{
	    cin>> ch;
	}
	cout<<fact(n-1);
	return(0);
}
